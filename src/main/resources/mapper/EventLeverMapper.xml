<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.dhcc.cn.framework.mapper.EventLeverMapper">

    <resultMap id="EventLever" type="com.dhcc.cn.framework.pojo.mysql.EventLever">
        <id column="id" property="id"/>
        <result column="name" property="name"/>
        <result column="code" property="code"/>
    </resultMap>

    <select id="findEventLeverById" resultType="string">
        select t.name from study.event_level as t where id=#{id}
    </select>

</mapper>